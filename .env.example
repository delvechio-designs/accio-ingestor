# accio-ingestor/.env.example

# App folders
WATCH_DIR=./incoming
PROCESSED_DIR=./processed
FAILED_DIR=./failed

# Slack
SLACK_WEBHOOK_URL=

# Accio
ACCIO_ENDPOINT=http://localhost:9876/ingest
ACCIO_TOKEN=

# AWS/S3
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_SESSION_TOKEN=
S3_BUCKET=
S3_PREFIX=ingests/
S3_SSE_KMS_KEY_ID=
S3_OBJECT_LOCK_MODE=GOVERNANCE
S3_OBJECT_LOCK_DAYS=730

# OCR
TESSERACT_CMD=

# Logging
LOG_FILE=app.log
LOG_JSONL=audit.jsonl
LOG_LEVEL=INFO

# Optional queue encryption key (not used for crypto here; reserved for future)
QUEUE_ENCRYPTION_KEY=
